<?xml version="1.0" encoding="UTF-8"?>
<utpsdoc type="config">
    <wcdmastyle>new</wcdmastyle>
    <function>
        <item name="SaveButton" value="true" />
    </function>
    <CDMADiagnostics>
        <item name="Carriers" show="false" />
        <item name="Messages in SIM/USIM" show="true" />
        <item name="Contacts in SIM/USIM" show="true" />
        <item name="Messages in device" show="true" />
        <item name="Contacts in device" show="true" />
        <item name="IMSI Number" show="true" />
        <item name="Firmware Version" show="true" />
        <item name="Hardware Version" show="true" />
        <item name="Voltage(Millivolt)" show="true" />
        <item name="PRL Version" show="true" />
        <item name="ESN" show="true" />
        <item name="MEID" show="true" />
    </CDMADiagnostics>
    <WCDMADiagnostics>
        <tab name="Device" show="true" resourceID="IDS_TAB_DEVICE">
            <item name="Device name" show="true" resourceID="IDS_ITEM_DEVICE_NAME" />
            <item name="Application port" show="true" resourceID="IDS_ITEM_APP_PORT" />
            <item name="Serial number" show="true" resourceID="IDS_ITEM_SN" />
            <item name="IMEI" show="true" resourceID="IDS_ITEM_IMEI" />
            <item name="Hardware version" show="true" resourceID="IDS_ITEM_HARDWARE_VER" />
            <item name="Fireware version" show="true" resourceID="IDS_ITEM_FIREWARE_VER" />
            <item name="Software version" show="true" resourceID="IDS_ITEM_SOFTWARE_VER" />
        </tab>
        <tab name="SIM/USIM" show="true" resourceID="IDS_TAB_SIM_USIM">
            <item name="IMSI number" show="false" resourceID="IDS_ITEM_IMSI" />
            <item name="Own number(MSISDN)" show="true" resourceID="IDS_ITEM_MSISDN" />
            <item name="Messages in SIM/USIM" show="true" resourceID="IDS_ITEM_SMS" />
            <item name="Contacts in SIM/USIM" show="true" resourceID="IDS_ITEM_CONTACT" />
            <item name="SMSC(Message center number)" show="true" resourceID="IDS_ITEM_SMSC" />
            <item name="PIN code status" show="true" resourceID="IDS_ITEM_PIN_STATUS">
                <lang value="Ready" resourceID="IDS_LANG_READY" />
                <lang value="PIN required" resourceID="IDS_LANG_PIN_REQUIRED" />
                <lang value="PUK required" resourceID="IDS_LANG_PUK_REQUIRED" />
                <lang value="PIN blocked" resourceID="IDS_LANG_PIN_BLOCKED" />
                <lang value="PIN2 required" resourceID="IDS_LANG_PIN2_REQUIRED" />
                <lang value="PUK2 required" resourceID="IDS_LANG_PUK2_REQUIRED" />
            </item>
            <item name="ICCID" show="false" resourceID="IDS_ITEM_ICCID" />
        </tab>
        <tab name="Network status" show="true" resourceID="IDS_TAB_NETWORK_STATUS">
            <item name="Network name" show="true" resourceID="IDS_ITEM_NETWORK_NAME" />
            <item name="Network type" show="false" resourceID="IDS_ITEM_NETWORK_TYPE" />
            <item name="Signal strength" show="false" resourceID="IDS_ITEM_SIGNAL" />
            <item name="RSSI" show="true" resourceID="IDS_ITEM_RSSI" />
            <item name="RSQUAL(BER)" show="false" resourceID="IDS_ITEM_BER" />
            <item name="RSCP" show="false" resourceID="IDS_ITEM_RSCP" />
            <item name="Ec/Io" show="false" resourceID="IDS_ITEM_ECIO" />
            <item name="Ec/No" show="false" resourceID="IDS_ITEM_ECNO" />
            <item name="Cell ID" show="true" resourceID="IDS_ITEM_CELLID" />
            <item name="LAC Code" show="false" resourceID="IDS_ITEM_LACCODE" />
            <item name="CS network registration" show="true" resourceID="IDS_ITEM_CS_REG">
                <lang value="Ready" resourceID="IDS_LANG_READY" />
                <lang value="Not registered" resourceID="IDS_LANG_NOT_REGISTERED" />
                <lang value="Registered" resourceID="IDS_LANG_REGISTERED" />
                <lang value="Registering" resourceID="IDS_LANG_REGISTERING" />
                <lang value="Registration denied" resourceID="IDS_LANG_REG_DENIED" />
                <lang value="Roaming" resourceID="IDS_LANG_ROAMING" />
            </item>
            <item name="PS network registration" show="true" resourceID="IDS_ITEM_PS_REG">
                <lang value="Ready" resourceID="IDS_LANG_READY" />
                <lang value="Not registered" resourceID="IDS_LANG_NOT_REGISTERED" />
                <lang value="Registered" resourceID="IDS_LANG_REGISTERED" />
                <lang value="Registering" resourceID="IDS_LANG_REGISTERING" />
                <lang value="Registration denied" resourceID="IDS_LANG_REG_DENIED" />
                <lang value="Roaming" resourceID="IDS_LANG_ROAMING" />
            </item>
            <item name="PS network attachment" show="true" resourceID="IDS_ITEM_PS_ATTACH">
                <lang value="Not attached" resourceID="IDS_LANG_NOT_ATTACHED" />
                <lang value="Attached" resourceID="IDS_LANG_ATTACHED" />
            </item>
            <item name="IPv4 address" show="false" resourceID="IDS_ITEM_IP" />
            <item name="DNS server" show="false" resourceID="IDS_ITEM_DNS" />
            <item name="RSRP" show="false" resourceID="IDS_ITEM_RSRP" />
            <item name="RSRQ" show="false" resourceID="IDS_ITEM_RSRQ" />
            <item name="PCI" show="false" resourceID="IDS_ITEM_PCI" />
            <item name="IPv6 address" show="false" resourceID="IDS_ITEM_IP6" />
        </tab>
        <tab name="Network settings" show="true" resourceID="IDS_TAB_NETWORK_SETTINGS">
            <item name="APN" show="true" resourceID="IDS_ITEM_APN" />
            <item name="Network selection mode" show="true" resourceID="IDS_ITEM_NETWORK_SEL_MODE">
                <lang value="Manual" resourceID="IDS_LANG_MANUAL" />
                <lang value="Automatic" resourceID="IDS_LANG_AUTOMATIC" />
            </item>
        </tab>
    </WCDMADiagnostics>
    <common>
        <function>
            <simmsgprompt>SIM/USIM</simmsgprompt>
        </function>
    </common>
    <!--customize name="T-Mobile_C105">
        <function>
            <simmsgprompt>SIM</simmsgprompt>
        </function>
    </customize-->
</utpsdoc>